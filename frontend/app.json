{
  "expo": {
    "name": "DataMate",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/exploration.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/exploration.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.siddharth2709.datamate"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/exploration.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.siddharth2709.datamate"
    },
    "web": {
      "favicon": "./assets/exploration.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "@react-native-voice/voice",
        {
          "microphonePermission": "Allow Voice to Text Tutorial to access the microphone",
          "speechRecognitionPermission": "Allow Voice to Text Tutorial to securely recognize user speech"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "1d5fbb0b-33fa-40e8-b54f-f5f9702f7ed7"
      }
    }
  }
}
